<p id="notice"><%= notice %></p>

<h1>Listing Server Log Entries</h1>

<table id="serverLogTable">
  <thead>
    <tr>
      <th>Entrytime</th>
      <th>Flags</th>
      <th>Source</th>
      <th>Message</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @server_log_entries.each do |server_log_entry| %>
      <tr>
        <td><%= server_log_entry.entrytime %></td>
        <td><%= server_log_entry.flags %></td>
        <td><%= server_log_entry.source %></td>
        <td><%= server_log_entry.message %></td>
        <td><%= link_to 'Show', server_log_entry %></td>
        <td><%= link_to 'Edit', edit_server_log_entry_path(server_log_entry) %></td>
        <td><%= link_to 'Destroy', server_log_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Server log entry', new_server_log_entry_path %>